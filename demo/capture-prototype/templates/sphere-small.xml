<krpano onstart="" showerrors="true" debugmode="true">

<control mousetype="drag2d" touchtype="drag2d"/>
<scene name="changed">
    <image>
        <sphere url="" />
    </image>
</scene>

<action name="setimage">
    set(scene[changed].content, "<image><sphere url="%1"/></image>");
    loadscene('changed');
</action>

</krpano>
